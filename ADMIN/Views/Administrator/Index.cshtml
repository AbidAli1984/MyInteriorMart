    <script src="~/Hubs-Signalr/AdminDashboard/UsersOnlineCategories.js"></script>
    <script src="~/Hubs-Signalr/AdminDashboard/UsersOnlineListing.js"></script>
    <script src="~/Hubs-Signalr/AdminDashboard/UsersOnlineDashboard.js"></script>
    <script src="~/Hubs-Signalr/AdminDashboard/GetAllConnectedUsers.js"></script>
    <script src="~/Hubs-Signalr/AdminDashboard/AllConnectedUsersMapHub.js"></script>
    <script src="~/js/notify.js"></script>
    <!-- End: -->
    <!-- Shafi: Create map using help from this url http://bl.ocks.org/d3noob/9150014 -->
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.4.0/dist/leaflet.css"
          integrity="sha512-puBpdR0798OZvTTbP4A8Ix/l+A4dHDD0DGqYW6RQ+9jxkRFclaxxQb/SJAWZfWAkuyeQUytO7+7N4QKrDh+drA=="
