<div class="row row-sm">
    <div class="col-xl-12">
        <div class="card">
            <div class="card-header pb-0">
                <div class="d-flex justify-content-between">
                    <h4 class="card-title mg-b-0">USERS ONLINE</h4>
                    <i class="mdi mdi-dots-horizontal text-gray"></i>
                </div>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table text-md-nowrap" id="allUsers">
                        <thead>
                            <tr>
                                <th>Image</th>
                                <th>Type</th>
                                <th>Username</th>
                                <th>Country</th>
                                <th>State</th>
                                <th>City</th>
                                <th>Pincode</th>
                                <th>Latitude & Longitude</th>
                                <th>Browsing</th>
                                <th>Referrer</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Image</th>
                                <th>Type</th>
                                <th>Username</th>
                                <th>Country</th>
                                <th>State</th>
                                <th>City</th>
                                <th>Pincode</th>
                                <th>Latitude & Longitude</th>
                                <th>Browsing</th>
                                <th>Referrer</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <!--/div-->
</div>

<div id='map' style='width: 100%; height: 600px;'></div>


