@page "/MyAccount/Register"
<AuthorizeView>
    <Authorized>
        <main>
            <div class="container margin_60_35">
                <div class="row">
                    <div class="col-lg-3">
                        <SideNavigation isVendor="@isVendor" currentPage="@BOL.Constants.Register" />
                    </div>

                    <div class="col-lg-9">
                        <div class="tab-content profile-sidebar-content" id="v-pills-tabContent">
                            <div class="tab-pane fade show active" id="v-pills-profile" role="tabpanel" aria-labelledby="v-pills-profile-tab">
                                <div class="add-review">
                                    <h5>Edit Profile</h5>
                                    <form class="icon-form-group">
                                        <div class="row">

                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Mobile Number</label>
                                                    <input class="form-control" type="text" name="number">
                                                    <i class="icon_phone" style="left: 10px;"></i>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label>
                                                    Email &nbsp; <a href="#verify_email" title="Verify Email" id="enquiry-in"
                                                                    class="text-success verify_email">Verify Email</a>
                                                </label>
                                                <input type="email" class="form-control" name="email" id="email">
                                                <i class="icon_mail_alt" style="top: 30px;"></i>
                                            </div>

                                            <div class="text-center col-12 mt-3">
                                                <input type="submit" value="Submit" class="btn btn-primary w-100">
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>
    </Authorized>
    <NotAuthorized>
        <UnAuthorizedUser />
    </NotAuthorized>
</AuthorizeView>