@page "/ManageListing/Enquiry"
<AuthorizeView>
    <Authorized>
        <div class="container my-5">
            <div class="row">
                <div class="col-md-12">
                    <div class="profile-sidebar-content">
                        <h4>Enquiry</h4>
                        <p class="add-lidting-title-from">
                            My Activity / Enquiry
                            <span>
                                <a class="back_btn mx-3" href="/MyAccount/MyActivity">Back</a>
                            </span>
                        </p>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Name</label>
                                    <input class="form-control" type="text" name="enquiry_name">
                                </div>
                                <div class="form-group">
                                    <label>Email</label>
                                    <input class="form-control" type="email" name="enquiry_email_name">
                                </div>
                                <div class="form-group">
                                    <label>Description</label>
                                    <textarea class="form-control" id="enquiry" name="enquiry"
                                              style="height:100px;"></textarea>
                                </div>
                                <div class="text-center mt-3">
                                    <input type="submit" value="Submit" class="btn btn-primary w-100">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </Authorized>
    <NotAuthorized>
        <UnAuthorizedUser />
    </NotAuthorized>
</AuthorizeView>